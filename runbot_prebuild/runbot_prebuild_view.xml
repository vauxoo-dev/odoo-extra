<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Pre-Builds Branch-->
        <record id="view_prebuild_branch_form" model="ir.ui.view">
            <field name="model">runbot.prebuild.branch</field>
            <field name="arch" type="xml">
                <form string="Pre-Build" version="7.0">
                    <sheet>
                        <group>
                            <field name="branch_id"/>
                            <field name="check_pr"/>
                            <field name="check_new_commit"/>
                            <field name="create_status_ok"/>
                            <field name="sha"/>
                            <field name="repo_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_prebuild_branch_tree" model="ir.ui.view">
            <field name="model">runbot.prebuild.branch</field>
            <field name="arch" type="xml">
                <tree string="Pre-Builds" editable="bottom">
                    <field name="branch_id"/>
                    <field name="check_pr"/>
                    <field name="check_new_commit"/>
                    <field name="create_status_ok"/>
                    <field name="sha"/>
                    <field invisible="1" name="repo_id"/>
                </tree>
            </field>
        </record>

        <!-- Pre-Builds -->
        <record id="view_prebuild_form" model="ir.ui.view">
            <field name="model">runbot.prebuild</field>
            <field name="arch" type="xml">
                <form string="Pre-Build">
                    <header>
                        <button name="create_main_build" type="object" string="Create main build"/>
                    </header>
                    <sheet>
                        <div class="oe_title oe_left">
                                <div class="oe_edit_only">
                                    <label for="sticky"/>
                                    <field name="sticky" string="Do you want it auto-test per commit? (sticky)"/>
                                </div>
                            <h1>
                               <field name="name" default_focus="1" placeholder="i.e.: 8.0-yourprebuild"/>
                            </h1>
                        </div>
                        <div class="oe_right oe_button_box" name="buttons">
                            <button class="oe_stat_button" icon="fa-list" name="get_builds_prebuild" type="object">
                                <div><strong>Builds</strong></div>
                            </button>
                        </div>
                        <group colspan="6">
                            <group cols="3" string="Main Configuration">
                                <field name="team_id"/>
                            </group>
                            <group cols="3" string="Extra Configuration">
                                <field name="pylint_conf_path"/>
                                <field name="lang"/>
                                <!--<field name="modules_to_exclude"/>-->
                            </group>
                        </group>
                        <separator string="Module to Install"/>
                        <field name="modules" nolabel="1"/>
                        <group colspan="6" string="Branches">
                            <div>
                                <p>
                                    Select the necessary branches one by one<br/>
                                    <strong>Hint:</strong> if you are looking for Vauxoo/lodigroup branch 8.0
                                    look for 8.0%lodigroup and you will find it.
                                </p>
                                <field name="module_branch_ids" nolabel="1"/>
                             </div>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_prebuild_tree" model="ir.ui.view">
            <field name="model">runbot.prebuild</field>
            <field name="arch" type="xml">
                <tree string="Pre-Builds">
                    <field name="name"/>
                    <field name="module_branch_ids"/>
                    <field name="sticky"/>
                    <field name="modules"/>
                    <field name="lang"/>
                    <field name="pylint_conf_path"/>
                    <!--<field name="modules_to_exclude"/>-->
                    <field name="script_prebuild"/>
                    <field name="script_posbuild"/>
                </tree>
            </field>
        </record>
        <record id="view_prebuild_search" model="ir.ui.view">
            <field name="model">runbot.prebuild</field>
            <field name="arch" type="xml">
                <search string="Search Pre-builds">
                    <field name="name"/>
                    <field name="sticky"/>
                    <field name="modules"/>
                    <field name="lang"/>
                    <field name="pylint_conf_path"/>
                    <!--
                    <field name="modules_to_exclude"/>
                        -->
                    <field name="script_prebuild"/>
                    <field name="script_posbuild"/>
                </search>
            </field>
        </record>
        <record id="action_prebuild" model="ir.actions.act_window">
            <field name="name">Pre-Builds</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">runbot.prebuild</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_prebuild" action="action_prebuild" parent="runbot.menu_runbot"/>

        <!-- Runbot Team -->

        <record id="view_runbot_team_tree" model="ir.ui.view">
            <field name="model">runbot.team</field>
            <field name="arch" type="xml">
                <tree string="Runbot Teams">
                    <field name="name"/>
                    <field name="privacy_visibility"/>
                    <field name="description"/>
                    <field name="jobs_running"/>
                </tree>
            </field>
        </record>
        <record id="view_runbot_team_form" model="ir.ui.view">
            <field name="model">runbot.team</field>
            <field name="arch" type="xml">
                <form string="team" version="7.0">
                    <sheet>
                        <h1><field name="name" placholder="i.e.: CustomerName"/></h1>
                        <div class="oe_right"><label for="privacy_visibility"/><field name="privacy_visibility"/></div>
                        <notebook>
                            <page string="General Information">
                                <group>
                                    <field string="Public Description" name="description"/>
                                </group>
                            </page>
                            <page string="Dashboard Info">
                                <field name="prebuild_ids" nolabel="1"/>
                            </page>
                            <page string="Look and Feel">
                                <group>
                                    <field name="color" placeholder="i.e: rgb(0,3,3) or #fff"/>
                                    <field name="bg_image_id"/>
                                    <field name="bg_image_url"/>
                                </group>
                            </page>
                            <page string="Security">
                                <field name="groups_id" nolabel="1"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_runbot_team" model="ir.actions.act_window">
            <field name="name">Team</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">runbot.team</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_runbotteam" action="action_runbot_team" parent="runbot.menu_runbot"/>

        <!--inherit runbot.build-->
        <record id="view_build_form_inh_prebuild_01" model="ir.ui.view">
            <field name="model">runbot.build</field>
            <field name="inherit_id" ref="runbot.view_build_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="prebuild_id"/>
                    <field name="team_id"/>
                    <field name="from_main_prebuild_ok"/>
                    <separator string="Build lines" colspan="4"/>
                    <field name="line_ids" readonly="1" nolabel="1" colspan="4">
                        <tree string="Build Lines">
                            <field name="branch_id"/>
                            <field name="short_sha"/>
                            <field name="date"/>
                            <field name="author"/>
                            <field name="subject"/>
                            <field name="committername"/>
                            <field name="reason_ok"/>
                            <field name="reason_pr_ok"/>
                            <field name="create_status_ok"/>
                        </tree>
                        <form string="Build Lines">
                            <field name="branch_id"/>
                            <field name="refname"/>
                            <field name="sha"/>
                            <field name="short_sha"/>
                            <field name="commit_url" widget="url"/>
                            <field name="date"/>
                            <field name="author"/>
                            <field name="subject"/>
                            <field name="committername"/>
                            <field name="prebuild_line_id"/>
                            <field name="reason_ok"/>
                            <field name="reason_pr_ok"/>
                            <field name="create_status_ok"/>
                        </form>
                    </field>
                </xpath>
                <xpath expr="//header/button[@name='reset']" position="after">
                    <button name="force_schedule" type="object" string="Force run build"/>
                    <button name="fetch_build_lines" type="object" string="Fetch build"/>
                    <button name="kill" type="object" string="Kill build"/>
                </xpath>
            </field>
        </record>
        <record id="view_build_tree_inh_prebuild_01" model="ir.ui.view">
            <field name="model">runbot.build</field>
            <field name="inherit_id" ref="runbot.view_build_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="after">
                    <field name="prebuild_id"/>
                </xpath>
            </field>
        </record>
        <record id="view_build_search_inh_prebuild_01" model="ir.ui.view">
            <field name="model">runbot.build</field>
            <field name="inherit_id" ref="runbot.view_build_search"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="after">
                    <field name="prebuild_id"/>
                    <field name="team_id"/>
                </xpath>
                <xpath expr="//filter[@string='Status']" position="after">
                    <filter string="Prebuild" domain="[]" context="{'group_by':'prebuild_id'}"/>
                    <filter string="Team" domain="[]" context="{'group_by':'team_id'}"/>
                </xpath>
            </field>
        </record>

        <record id="view_repo_form_inh_team_01" model="ir.ui.view">
            <field name="model">runbot.repo</field>
            <field name="inherit_id" ref="runbot.view_repo_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='url']" position="after">
                    <field name="team_id"/>
                </xpath>
            </field>
        </record>
    <!-- Templates -->
    <template id="runbot_teams" name="Teams">
        <section class="s_images_captions_fw">
            <div class="container-fluid">
                    <t t-foreach='teams' t-as='team' class="row">
                    <div class="media">
                        <a class="pull-left" href="#">
                            <img class="media-object dp img-circle" t-att-src="team.bg_image_url" style="width: 100px;height:100px;"/>
                        </a>
                        <div class="media-body">
                            <h4 class="media-heading"><t t-raw="team.name"/><small></small></h4>
                            <h5>Prebuilds <t t-raw="len(team.prebuild_ids)"/></h5>
                            <hr style="margin:8px auto"/>

                            <a t-attf-href="/runbot/team/{{slug(team)}}"><span class="label label-info"><i class="fa fa-search"></i>Enter Team</span></a>
                            <span class="label label-default">jQuery</span>
                            <span class="label label-info">CakePHP</span>
                            <span class="label label-default">Android</span>
                        </div>
                    </div>
                    </t>
            </div>
        </section>
    </template>
    <template id="runbot_home" name="Vauxoo Runbot">
        <t t-call='website.layout'>
            <div id="wrap" class="oe_structure oe_empty">
                <t t-call="runbot_prebuild.runbot_teams"></t>
            </div>
        </t>
    </template>
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <script src="/runbot_prebuild/static/src/ext.js" type="type/javascript"/>
            <link href="/runbot_prebuild/static/src/css/runbot_css.css" rel="stylesheet"/>
        </xpath>
    </template>
    <template id="runbot_prebuild.runbot_navbar" name="Runbot Navbar">
      <t t-if="repo">
        <div class="btn-group btn-group-xs" role="group">
            <div class="btn-group">
            <a href="#" class="btn btn-info btn-xs dropdown-toggle" data-toggle="dropdown"><b><i class="fa fa-users"></i> <t t-esc="team and team.name or 'Pick a Team'"/></b><b class="caret"></b></a>
            <ul class="dropdown-menu">
              <t t-foreach='teams' t-as='team'>
                  <li><a t-attf-href="/runbot/team/{{slug(team)}}"><span t-esc="team.name"/></a></li>
              </t>
            </ul>
            </div>
            <div class="btn-group">
            <a href="#" class="btn btn-info btn-xs dropdown-toggle" data-toggle="dropdown"><i class="fa fa-filter"></i><span> Filter</span><b class="caret"></b></a>
            <ul class="dropdown-menu">
              <li t-if="filters['pending']=='0'"><a t-att-href="qu(pending=1)">Pending</a></li>
              <li t-if="filters['pending']=='1'"><a t-att-href="qu(pending='0')"><i class="fa fa-check"/> Pending</a></li>
              <li t-if="filters['testing']=='0'"><a t-att-href="qu(testing=1)">Testing</a></li>
              <li t-if="filters['testing']=='1'"><a t-att-href="qu(testing='0')"><i class="fa fa-check"/> Testing</a></li>
              <li t-if="filters['running']=='0'"><a t-att-href="qu(running=1)">Running</a></li>
              <li t-if="filters['running']=='1'"><a t-att-href="qu(running='0')"><i class="fa fa-check"/> Running</a></li>
              <li t-if="filters['done']=='0'"><a t-att-href="qu(done=1)">Done</a></li>
              <li t-if="filters['done']=='1'"><a t-att-href="qu(done='0')"><i class="fa fa-check"/> Done</a></li>
              <li class="divider"></li>
              <li t-att-class="'active' if limit=='100' else ''"><a t-att-href="qu(limit=100)">Show last 100</a></li>
              <li t-att-class="'active' if limit=='1000' else ''"><a t-att-href="qu(limit=1000)">Show last 1000</a></li>
              <li t-att-class="'active' if limit=='10000' else ''"><a t-att-href="qu(limit=10000)">Show last 10000</a></li>
            </ul>
            </div>
        </div>
        <!-- TODO: Now it is not working and only brings noise to the view.
            <form class="form-inline" role="search" t-att-action="qu(search='')" method="get">
              <div class="input-group">
                <input type="search" name="search" class="form-control" placeholder="Search" t-att-value="search"/>
                <button type="submit" class="input-group-addon btn btn-xs btn-default">Search</button>
              </div>
            </form>
        -->
      </t>
    </template>
    <template id="runbot_prebuild" inherit_id="runbot.repo" name="Runbot PreBuild">
        <xpath expr="//div[@class='container-fluid']" position="before">
        </xpath>
        <xpath expr="//div[@class='col-md-12']//nav" position="replace">
        </xpath>
        <xpath expr="//table/tr/th" position="replace">
            <th>
                <span>Team: </span>
                <t t-call="runbot_prebuild.runbot_navbar"/>
            </th>
        </xpath>
        <xpath expr="//table/tr/td" position="replace">
            <td colspan="4">
                <span><b>Statuses</b> </span>
                <t t-call='runbot_prebuild.runbot_status'/>
            </td>
        </xpath>
        <xpath expr="//table/tr[@t-as='br']//br" position="replace">
        </xpath>
        <xpath expr="//table/tr[@t-as='br']//small" position="replace">
        </xpath>
        <xpath expr="//table/tr[@t-as='br']//div[@class='btn-group btn-group-xs']" position="replace">
        </xpath>
        <xpath expr="//t[@t-as='bu']" position="replace">
            <t t-foreach="br['builds']" t-as="bu">
                <t t-if="bu['state']=='pending'"><t t-set="klass">default</t></t>
                <t t-if="bu['state']=='testing'"><t t-set="klass">info</t></t>
                <t t-if="bu['state'] in ['running','done'] and bu['result'] == 'ko'"><t t-set="klass">danger</t></t>
                <t t-if="bu['state'] in ['running','done'] and bu['result'] == 'warn'"><t t-set="klass">warning</t></t>
                <t t-if="bu['state'] in ['running','done'] and bu['result'] == 'ok'"><t t-set="klass">success</t></t>
                <t t-if="bu['state'] in ['running','done'] and bu['result'] == 'skipped'"><t t-set="klass">default</t></t>
                <t t-if="bu['state'] in ['running','done'] and bu['result'] == 'killed'"><t t-set="klass">killed</t></t>
                <td t-attf-class="{{klass}}">
                    <div style="padding: 5px;">
                        <t t-call="runbot.build_button"><t t-set="klass">btn-group-sm</t></t>
                        <t t-if="bu['prebuild_id'].name">
                            <t t-call="runbot.build_button"></t>
                            <div class="list-group builds">
                                <t t-foreach="bu['build'].line_ids" t-as="build_line">
                                    <a t-att-class="(build_line.reason_ok and 'active ' or '') + 'list-group-item '" t-att-title="build_line.branch_id.complete_name"  data-toggle="tooltip" data-placement="top" t-att-href="build_line.commit_url" target="_blank">
                                        <i class="fa fa-external-link"><span t-esc="build_line.short_sha"/></i></a>
                                </t>
                            </div>
                            <t t-call="runbot.build_name"/>
                        </t>
                    </div>
                </td>
            </t>
        </xpath>
        <xpath expr="//td//b[@t-esc]" position="replace">
            <t t-set="sticky_test"
                t-value="br['builds'] and br['builds'][0]['prebuild_id'] and br['builds'][0]['prebuild_id'].sticky and br['builds'][0]['fmpo']">
            </t>
            <t t-set="manual_test"
                t-value="br['builds'] and br['builds'][0]['prebuild_id'] and not br['builds'][0]['prebuild_id'].sticky and br['builds'][0]['fmpo']">
            </t>
            <t t-set="pr_test"
                t-value="br['builds'] and br['builds'][0]['prebuild_id'] and br['builds'][0]['prebuild_id'].sticky and not br['builds'][0]['fmpo']">
            </t>
            <t t-set="bname" t-value="len(br['builds'][0]['prebuild_id'].name) >= 20 and br['builds'][0]['prebuild_id'].name[:19]+'...' or br['builds'][0]['prebuild_id'].name"/>
            <t t-if="sticky_test">
                <t t-set="link_unique" t-value="str(br['builds'][0]['build'][0].prebuild_id.id)"/>
                <a class="btn btn-xs btn-link" t-att-href="'#' + link_unique" t-att-id="link_unique" t-att-title="br['builds'][0]['build'][0].name">
                <i class="fa fa-star"></i> <span t-esc="bname"/></a>
            </t>
            <t t-if="manual_test">
                <t t-set="link_unique" t-value="str(br['builds'][0]['build'][0].prebuild_id.id)"/>
                <a class="btn btn-xs btn-link" t-att-href="'#' + link_unique" t-att-id="link_unique" t-att-title="br['builds'][0]['build'][0].name">
                <i class="fa fa-hand-o-right"></i> <span t-esc="bname"/></a>
            </t>
            <t t-if="pr_test">
                <t t-set="link_pull_unique" t-value="str(br['builds'][0]['build'][0].prebuild_id.id) + '_'.join(br['builds'][0]['build'].branch_id.name.split('/')[1:])"/>
                <a class="btn btn-xs btn-link" t-att-href="'#' + link_pull_unique" t-att-id="link_pull_unique" t-att-title="br['builds'][0]['build'][0].name">
                     <i class="fa fa-code-fork"></i><span t-esc="bname"/>
                </a>
                <a class="btn btn-xs btn-link pull-right" t-att-href="'#' + link_pull_unique" t-att-id="link_pull_unique" t-att-title="br['builds'][0]['build'][0].name">
                    <sup><i class="fa fa-github"></i> <span t-esc="'/'.join(br['builds'][0]['build'].branch_id.name.split('/')[1:])"/></sup>
                </a>
            </t>
            <div class="list-group builds">
                <t t-foreach="br['builds'][0]['prebuild_id'].module_branch_ids" t-as="branch_id">
                    <a class="list-group-item" t-att-href="branch_id.branch_id.branch_url" target="_NEW">
                        <t t-set="burl" t-value="branch_id.branch_id.repo_id.url"/>
                        <t t-if="burl.startswith('https:///github')">
                            <i class="fa fa-github" title="Code hosted on github.com"> <span t-esc="branch_id.branch_id.complete_name"/></i>
                        </t>
                        <t t-if="burl.startswith('https:///bitbucket')">
                            <i class="fa fa-bitbucket" title="Code hosted in bitbucket.org"> <span t-esc="branch_id.branch_id.complete_name"/></i>
                        </t>
                        <t t-if="burl.startswith('file:///')">
                            <i class="fa fa-linux" title="Code hosted in launchpad.net"> <span t-esc="branch_id.branch_id.complete_name"/></i>
                        </t>
                        <t t-if="burl.startswith('/')">
                            <i class="fa fa-file" title="Code hosted locally"> <span t-esc="branch_id.branch_id.complete_name"/></i>
                        </t>
                    </a>
                </t>

            </div>
        </xpath>
        <xpath expr='//t[@t-call="runbot.build_button"]' position="replace">
            <t t-set="klass">btn-group-xs</t>
        </xpath>
    </template>
    <template id="runbot_prebuild.runbot_status" name="Runbot Status">
        <t  t-foreach="host_stats" t-as="hs">
        <span class="label label-default">
            <t t-esc="hs['host']"/>: <t t-esc="hs['testing']"/> testing, <t t-esc="hs['running']"/> running
        </span>&amp;nbsp;
        </t>
        <span class="label label-info">Pending: <t t-esc="pending_total"/></span>
    </template>
    <template id="runbot_inherit_build" inherit_id="runbot.build" name="Runbot Build">
        <xpath expr="//a[1]" position="replace">
            <t t-if="repo and not team">
                <a class="navbar-brand" t-attf-href="/runbot/repo/#{ slug(repo) }"><b><t t-esc="repo.base"/></b></a>
            </t>
            <t t-if="team">
                <a class="navbar-brand" t-attf-href="/runbot/team/#{ slug(team) }"><b><t t-esc="team.name"/></b></a>
            </t>
        </xpath>
    </template>

    <record id="view_branch_search_inh_repository" model="ir.ui.view">
        <field name="model">runbot.branch</field>
        <field name="inherit_id" ref="runbot.view_branch_search"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="repo_id" filter_domain="[('repo_id.name','ilike',self),]"/>
                <field name="branch_base_id" filter_domain="[('branch_base_id.name','ilike',self),]"/>
                <field name="full_name"/>
            </xpath>
            <xpath expr="//filter[@string='Owner']" position="after">
                <filter string="Branch Base" domain="[]" context="{'group_by':'branch_base_id'}"/>
            </xpath>
        </field>
    </record>

    <record id="view_repo_form_inh_button_branch" model="ir.ui.view">
        <field name="model">runbot.repo</field>
        <field name="inherit_id" ref="runbot.view_repo_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="before">
                <header>
                    <button name="get_branch_repo" string="Branches" type="object"/>
                    <button name="get_prebuild_repo" string="Prebuilds" type="object"/>
                </header>
            </xpath>
        </field>
    </record>

    <record id="view_branch_form_inh_full_name" model="ir.ui.view">
        <field name="model">runbot.branch</field>
        <field name="inherit_id" ref="runbot.view_branch_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="full_name"/>
            </xpath>
        </field>
    </record>

    <record id="view_branch_tree_inh_full_name" model="ir.ui.view">
        <field name="model">runbot.branch</field>
        <field name="inherit_id" ref="runbot.view_branch_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="full_name"/>
            </xpath>
        </field>
    </record>

    <template id="build_button" inherit_id="runbot.build_button" name="Pre Build Button">
        <xpath expr="//div[@t-attf-class='pull-right']" position="replace">
            <div t-attf-class="btn-group {{klass}}">
                <a t-att-href="'#'+bu['dest']" t-att-id="bu['dest']" class="btn btn-default btn-xs"><t t-esc="bu['dest']"/></a>
                <a t-if="bu['state']=='running'" target="_blank" t-attf-href="http://{{bu['domain']}}/?db={{bu['dest']}}-all" class="btn btn-primary btn-xs"><i class="fa fa-sign-in"/></a>
                <a t-attf-href="/runbot/build/{{bu['id']}}" class="btn btn-default btn-xs"><i class="fa fa-file-text-o"/></a>
                <a t-attf-href="https://#{repo.base}/commit/#{bu['name']}" class="btn btn-default btn-xs"><i class="fa fa-github"/></a>
                <button class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown"><i class="fa fa-cog"/><span class="caret"></span></button>
                <ul class="dropdown-menu" role="menu">
                    <li t-if="bu['result']=='skipped'">
                        <a href="#" class="runbot-rebuild" t-att-data-runbot-build="bu['id']">Force Build <i class="fa fa-level-up"></i></a>
                    </li>
                    <t t-if="bu['state']=='running'">
                        <li><a t-attf-href="http://{{bu['host']}}/?db={{bu['real_dest']}}-all">Connect all <i class="fa fa-sign-in"></i></a></li>
                        <li><a t-attf-href="http://{{bu['host']}}/?db={{bu['real_dest']}}-base">Connect base <i class="fa fa-sign-in"></i></a></li>
                        <li><a t-attf-href="http://{{bu['host']}}/">Connect <i class="fa fa-sign-in"></i></a></li>
                    </t>
                    <li t-if="bu['state'] in ['done','running'] and bu_index==0">
                        <a href="#" class="runbot-rebuild" t-att-data-runbot-build="bu['id']">Rebuild <i class="fa fa-refresh"/></a>
                    </li>
                    <li t-if="bu['state']!='testing' and bu['state']!='pending'" class="divider"></li>
                    <li><a t-attf-href="/runbot/build/{{bu['id']}}">Logs <i class="fa fa-file-text-o"/></a></li>
                    <li><a t-attf-href="http://{{bu['host']}}/runbot/static/build/#{bu['real_dest']}/logs/job_10_test_base.txt">Full base logs <i class="fa fa-file-text-o"/></a></li>
                    <li><a t-attf-href="http://{{bu['host']}}/runbot/static/build/#{bu['real_dest']}/logs/job_20_test_all.txt">Full all logs <i class="fa fa-file-text-o"/></a></li>
                    <li><a t-attf-href="/runbot/static/build/#{bu['real_dest']}/logs/job_25_pylint.txt">Pylint logs <i class="fa fa-file-text-o"/></a></li>
                    <li t-if="bu['state']!='pending'" class="divider"></li>
                    <li><a t-attf-href="{{br['branch'].branch_url}}">Branch or pull <i class="fa fa-github"/></a></li>
                    <li><a t-attf-href="https://{{repo.base}}/commit/{{bu['name']}}">Commit <i class="fa fa-github"/></a></li>
                    <li><a t-attf-href="https://{{repo.base}}/compare/{{br['branch'].branch_name}}">Compare <i class="fa fa-github"/></a></li>
                    <!-- TODO branch.pull from -->
                    <li class="divider"></li>
                    <li class="disabled"><a href="#">Runtime: <t t-esc="bu['job_time']"/>s</a></li>
                    <li class="disabled"><a href="#">Port: <t t-esc="bu['port']"/></a></li>
                    <li class="disabled"><a href="#">Age: <t t-esc="bu['job_age']"/></a></li>
                </ul>
            </div>
        </xpath>
    </template>

    </data>
</openerp>
